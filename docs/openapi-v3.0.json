  "components": {
    "schemas": {
      "Student": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string",
            "maxLength": 100,
            "minLength": 2
          },
          "email": {
            "type": "string",
            "minLength": 1
          },
          "dob": {
            "type": "string",
            "format": "date"
          }
        },
        "required": [
          "dob",
          "email",
          "name"
        ]
      },
      "Address": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "street": {
            "type": "string",
            "maxLength": 100,
            "minLength": 3
          },
          "city": {
            "type": "string",
            "maxLength": 50,
            "minLength": 2
          },
          "zip": {
            "type": "string",
            "minLength": 1,
            "pattern": "^[0-9]{5}(-[0-9]{4})?$"
          }
        },
        "required": [
          "city",
          "street",
          "zip"
        ]
      }
    }
